void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        // step 1: put all elements in nums1
        int j = 0;
        for(int i = m; i < m+n; i++){
            nums1[i] = nums2[j];
            j++;
        }
        // step 2: sort nums1
        sort(nums1.begin(), nums1.end());
    }
