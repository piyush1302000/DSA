bool find132pattern(vector<int>& nums) {
        stack <int>s;
        int n=nums.size();
        int second =INT_MIN;
        for(int i=n-1;i>=0;i--)
        {
            if(nums[i]<second)
            {
                return true;
            }
            while(!s.empty() && nums[i]>s.top())
            {
                second=s.top();
                s.pop();
            }
            s.push(nums[i]);
        }
        return false;
    }
